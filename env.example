# Vite Environment Variables for Safe Desktop
# Copy this file to .env and fill in your values
# All variables must be prefixed with VITE_ to be accessible in the renderer process

# Ethereum RPC URL (e.g., Alchemy, Infura)
VITE_RPC_URL=

# Safe contract address
VITE_SAFE_ADDRESS=

# Chain ID (11155111 for Sepolia, 1 for Mainnet)
VITE_CHAIN_ID=

# Wallet private keys (PoC only - use secure keystore in production!)
VITE_WALLET_A_PK=
VITE_WALLET_B_PK=

